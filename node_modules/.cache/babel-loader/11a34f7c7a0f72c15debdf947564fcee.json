;AAEA,QAAA,IAAA,EAAA;AACA,UAAA,IAAA,EAAA,IADA;AAEA,UAAA,KAAA,EAAA,KAFA;AAGA,UAAA,KAAA,EAAA,KAHA;AAIA,UAAA,QAAA,EAAA,QAJA;AAKA,UAAA,KAAA,EAAA;AALA;AAFA,OAAA;AAUA,WAAA,OAAA,CAAA,OAAA,EAZA,CAcA;;AACA,WAAA,QAAA,GAAA;AACA,QAAA,IAAA,EAAA,EADA;AAEA,QAAA,KAAA,EAAA,EAFA;AAGA,QAAA,OAAA,EAAA,IAHA;AAIA,QAAA,KAAA,EAAA,IAJA;AAKA,QAAA,QAAA,EAAA,CALA;AAMA,QAAA,KAAA,EAAA;AANA,OAAA;AAQA;AA9BA;AArBA,CAAA","sourcesContent":["<template>\n  <div class=\"home-view-container\">\n    <h1>Buy a new fone.</h1>\n    {{ getAllSams.length }}\n    {{ foneCount }}\n\n    <button @click=\"toggleFoneForm\" class=\"btn btn-primary\">Add New Fone</button>\n\n    <b-form @submit.prevent=\"handleSubmit\" v-if=\"showFoneForm\">\n\n      <b-form-group id=\"input-group-2\" label=\"Fone Name:\" label-for=\"input-2\">\n        <b-form-input\n          id=\"input-2\"\n          v-model=\"formData.name\"\n          required\n          placeholder=\"Enter name\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-3\" label=\"Species:\" label-for=\"input-3\">\n        <b-form-select\n          id=\"input-3\"\n          v-model=\"formData.species\"\n          :options=\"['Sams', 'Apple']\"\n          required\n        ></b-form-select>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-3\" label=\"Color:\" label-for=\"input-3\">\n        <b-form-select\n          id=\"input-3\"\n          v-model=\"formData.color\"\n          :options=\"['Black', 'White', 'Pink', 'Gold', 'Gray']\"\n          placeholder=\"Choose Color\"\n          required\n        ></b-form-select>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-2\" label=\"Fone Quantity:\" label-for=\"input-2\">\n        <b-form-input\n          id=\"input-2\"\n          v-model=\"formData.quantity\"\n          required\n          placeholder=\"Enter Quantity\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-2\" label=\"Fone Price:\" label-for=\"input-2\">\n        <b-form-input\n          id=\"input-2\"\n          v-model=\"formData.price\"\n          required\n          placeholder=\"Enter Price\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-2\" label=\"Fone Model:\" label-for=\"input-2\">\n        <b-form-input\n          id=\"input-2\"\n          v-model=\"formData.model\"\n          required\n          placeholder=\"Enter model\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n      <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\n    </b-form>\n\n  </div>\n</template>\n\n<script>\nimport { mapActions,mapGetters } from 'vuex'\nexport default {\n  name: 'home',\n  data(){\n    return {\n      showFoneForm: false,\n      formData:{\n        name:'',\n        model:'',\n        species:null,\n        color:null,\n        quantity:0,\n        price:0\n      }\n    }\n  },\n  computed: {\n    ...mapGetters([\n      'foneCount',\n      'getAllSams'\n    ])\n  },\n  methods: {\n    ...mapActions([\n      'addFone'\n    ]),\n    toggleFoneForm() {\n      this.showFoneForm =! this.showFoneForm\n    },\n    handleSubmit(){\n      const { species, name, model, color, quantity, price } = this.formData\n      const payload = {\n        species,\n        fone: {\n          name,\n          model,\n          color,\n          quantity,\n          price\n        }\n      }\n      this.addFone(payload)\n\n      //reset form\n      this.formData = {\n        name:'',\n        model:'',\n        species:null,\n        color:null,\n        quantity:0,\n        price:0\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}